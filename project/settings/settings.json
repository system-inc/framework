{
	"title": "Project",
	"modules": {
		"webServer": {
			"protocol": "http",
			"ports": 8080,
			"router": {
				"routes": [
					{
						"controllerName": "Main",
						"methodName": "main",
						"protocol": "*",
						"port": "*",
						"method": "*",
						"expression": "/",
						"data": {
							"rootData": "root!"
						},
						"children": [
							{
								"methodName": "contact",
								"expression": "contact/?",
								"data": {
									"contactData": "contact!"
								}
							},
							{
								"methodName": "legal",
								"expression": "legal/?",
								"children": [
									{
										"methodName": "legalTermsOfService",
										"expression": "terms-of-service/?"
									}
								]
							}
						]
					}
				]
			}
		}
	}
}